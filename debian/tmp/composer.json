{
  "name": "vitexsoftware/multiflexi-scheduler",
  "description": "Tool used to schedule AbraFlexi Jobs",
  "type": "project",
  "require": {
    "deb/multiflexi-core": "*",
    "dragonmantank/cron-expression": "^3.0@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "info@vitexsoftware.cz"
    }
  ],
  "config": {
    "vendor-dir": "/var/lib/composer/multiflexi-scheduler"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "MultiFlexi\\": "./MultiFlexi/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/MultiFlexi/"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseFluentPDO/"
    }
  ],
  "version": "2.1.0"
}
