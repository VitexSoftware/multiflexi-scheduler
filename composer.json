    "type": "project",
    "require": {
        "vitexsoftware/multiflexi-core": "dev-main",
        "dragonmantank/cron-expression": "3.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MultiFlexi\\Scheduler\\": "src/"
        }
    },
